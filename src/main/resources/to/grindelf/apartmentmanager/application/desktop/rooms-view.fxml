<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.text.Text?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" hgap="10" vgap="10"
          fx:controller="to.grindelf.apartmentmanager.application.desktop.controllers.RoomsController">

    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Text GridPane.columnIndex="0" fx:id="text" GridPane.rowIndex="4" GridPane.columnSpan="2">
        Soon you will be able to add new rooms here and manage their list.
    </Text>

    <Button text="←" fx:id="goBackButton" onAction="#goBack" GridPane.columnIndex="0" GridPane.rowIndex="3" />

</GridPane>
